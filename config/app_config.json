{
  "environment": "development",
  "debug": true,
  "host": "0.0.0.0",
  "port": 8000,
  "workers": 1,
  "reload": true,
  "database": {
    "type": "sqlite",
    "host": null,
    "port": null,
    "database": "data/proxy_collector_development.db",
    "username": null,
    "password": null,
    "url": null,
    "pool_size": 10,
    "max_overflow": 20,
    "pool_timeout": 30,
